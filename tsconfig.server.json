{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "umd",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noImplicitReturns": false
  },
  "include": [
    "./server/**/*",
    "./lib/**/*",
    "config/**/*"
  ]
}
